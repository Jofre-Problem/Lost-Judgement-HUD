"UnlitGeneric"
{
 	"$basetexture"	"vgui/replay/thumbnails/radar/blip"
 	"$translucent"	1
 	"$vertexcolor"	1
 	"$vertexalpha"	1
		"$angle" "0.0"
	"$translate" "[0.0 0.0]"
	"$center" "[0.5 0.5]"

	"$speed" "0"
	"Proxies"
	{
		"LinearRamp"
		{
			"rate" "40"
			"initialValue" "0.0"
			"resultVar" "$angle"
		}

		"TextureTransform"
		{
			"translateVar" "$translate"
			"rotateVar" "$angle"
			"centerVar" "$center"
			"resultVar" "$basetexturetransform"
		}
	}
}